            <operator id="AVMNOperator.MC1R_protein_trimmed" spec="kernel.AdaptableVarianceMultivariateNormalOperator" allowNonsense="true" beta="0.05" burnin="400" initial="800" weight="0.1">
                <transformations id="AVMNSumTransform.MC1R_protein_trimmed" spec="operator.kernel.Transform$LogConstrainedSumTransform"/>
                <transformations id="AVMNLogTransform.MC1R_protein_trimmed" spec="operator.kernel.Transform$LogTransform">
